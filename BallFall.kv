#:kivy 1.0.9
#:import ReferenceListProperty kivy.properties.ReferenceListProperty
<Ball>:
    size: self.d, self.d
    canvas:
        Color:
            rgba: (1,0,0,1)
        Ellipse:
            pos: self.pos
            size: self.size

<GameWidget>:
    canvas:
        Rectangle:
            pos: self.pos
            size: self.size
